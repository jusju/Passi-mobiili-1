<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/content_saarinakyma"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="fi.softala.tyokykypassi.activities.SaarinakymaActivity"
    android:background="@drawable/kartta_tausta"
    >

    <include
        layout="@layout/tool_bar"
        android:id="@+id/toolbar"
        android:layout_width="0dp"
        android:layout_height="60dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent">
    </include>

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/kartta_nappi_1"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="24dp"
        app:layout_constraintTop_toBottomOf="@+id/toolbar"
        android:id="@+id/imageButton2" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/kartta_nappi_2"
        android:layout_marginTop="24dp"
        app:layout_constraintTop_toBottomOf="@+id/imageButton2"
        android:layout_marginEnd="16dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginRight="16dp"
        android:id="@+id/imageButton4" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/kartta_nappi_3"
        android:layout_marginTop="24dp"
        app:layout_constraintTop_toBottomOf="@+id/imageButton4"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="16dp"
        android:id="@+id/imageButton5" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/kartta_nappi_4"
        android:layout_marginTop="24dp"
        app:layout_constraintTop_toBottomOf="@+id/imageButton5"
        android:layout_marginEnd="16dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginRight="16dp"
        android:id="@+id/imageButton6" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/kartta_nappi_5"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="24dp"
        app:layout_constraintTop_toBottomOf="@+id/imageButton6"
        android:id="@+id/imageButton" />



    <TextView
        android:text="Ammatin työkykyvalmiudet"
        android:lines="2"
        android:maxLines="2"
        android:maxWidth="150dip"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView6"
        app:layout_constraintTop_toTopOf="@+id/imageButton2"
        app:layout_constraintRight_toRightOf="@+id/imageButton2"
        android:layout_marginTop="16dp"
        app:layout_constraintLeft_toLeftOf="@+id/imageButton2"
        android:layout_marginStart="50dp"
        android:layout_marginLeft="50dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageButton2"
        android:layout_marginBottom="16dp"
        android:textSize="15sp"
        android:textStyle="normal|bold"
        android:gravity="center"/>

    <TextView
        android:text="Työkykyä edistävä liikunta"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:lines="2"
        android:maxLines="2"
        android:maxWidth="150dip"
        android:id="@+id/textView7"
        app:layout_constraintTop_toTopOf="@+id/imageButton4"
        app:layout_constraintLeft_toLeftOf="@+id/imageButton4"
        app:layout_constraintRight_toRightOf="@+id/imageButton4"
        app:layout_constraintBottom_toBottomOf="@+id/imageButton4"
        android:layout_marginEnd="50dp"
        android:layout_marginRight="50dp"
        android:layout_marginTop="16dp"
        android:layout_marginBottom="16dp"
        android:textStyle="normal|bold"
        android:textSize="15sp"
        android:gravity="center"/>

    <TextView
        android:text="Terveysosaaminen"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView8"
        android:lines="2"
        android:maxLines="2"
        android:maxWidth="150dip"
        app:layout_constraintLeft_toLeftOf="@+id/imageButton5"
        android:layout_marginStart="50dp"
        android:layout_marginLeft="50dp"
        app:layout_constraintRight_toRightOf="@+id/imageButton5"
        app:layout_constraintBottom_toBottomOf="@+id/imageButton5"
        app:layout_constraintTop_toTopOf="@+id/imageButton5"
        android:layout_marginTop="16dp"
        android:layout_marginBottom="16dp"
        android:textStyle="normal|bold"
        android:textSize="15sp"
        android:gravity="center"/>

    <TextView
        android:text="Harrastuneisuus ja yhteistyötaidot"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView9"
        android:lines="2"
        android:maxLines="2"
        android:maxWidth="150dip"
        app:layout_constraintLeft_toLeftOf="@+id/imageButton6"
        app:layout_constraintTop_toTopOf="@+id/imageButton6"
        app:layout_constraintBottom_toBottomOf="@+id/imageButton6"
        app:layout_constraintRight_toRightOf="@+id/imageButton6"
        android:layout_marginEnd="50dp"
        android:layout_marginRight="50dp"
        android:layout_marginTop="16dp"
        android:layout_marginBottom="16dp"
        android:textStyle="normal|bold"
        android:textSize="15sp"
        android:gravity="center"/>

    <TextView
        android:text="Työkykyvalmiuksien vahvistaminen"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView11"
        android:lines="2"
        android:maxLines="2"
        android:maxWidth="150dip"
        app:layout_constraintRight_toRightOf="@+id/imageButton"
        app:layout_constraintLeft_toLeftOf="@+id/imageButton"
        android:layout_marginStart="50dp"
        android:layout_marginLeft="50dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageButton"
        app:layout_constraintTop_toTopOf="@+id/imageButton"
        android:layout_marginTop="16dp"
        android:layout_marginBottom="16dp"
        android:textStyle="normal|bold"
        android:textSize="15sp"
        android:gravity="center"/>



</android.support.constraint.ConstraintLayout>
